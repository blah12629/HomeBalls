@using CEo.Pokemon.HomeBalls.App.Components.Settings
@inherits HomeBallsMenuLayout
@inject IHomeBallsAppSettings Settings

@{ base.BuildRenderTree(__builder); }

@code
{
    protected override RenderFragment Body => builder =>
    {
        builder.OpenComponent<HomeBallsAppSettings>(0);
        builder.CloseComponent();
    };

    protected override IHomeBallsAppTab Tab => Settings;
}